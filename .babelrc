{
  "plugins": [
    "transform-decorators-legacy", 
    "transform-remove-strict-mode",
    "transform-class-properties",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ]
}